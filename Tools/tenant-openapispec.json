                      },
                      "TenantId": {
                        "type": "string",
                        "description": "Tenant ID"
                      },
                      "SubscriptionId": {
                        "type": "string",
                        "description": "Subscription ID"
                      },
                      "SkuId": {
                        "type": "string",
                        "description": "SKU ID"
                      },
                      "SkuName": {
                        "type": "string",
                        "description": "SKU name"
                      },
                      "Status": {
                        "type": "string",
                        "description": "Subscription status"
                      },
                      "Quantity": {
                        "type": "integer",
                        "description": "License quantity"
                      },
                      "BillingCycle": {
                        "type": "string",
