{
  "version": "2.0",
  "managedDependency": {
    "enabled": false
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:10:00",
  "extensions": {
    "durableTask": {
      "maxConcurrentActivityFunctions": 5,
      "maxConcurrentOrchestratorFunctions": 2,
      "tracing": {
        "distributedTracingEnabled": true,
        "version": "V2"
      }
    }
  }
}