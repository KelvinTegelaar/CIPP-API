{
  "scriptFile": "../Modules/CippEntrypoints/CippEntrypoints.psm1",
  "entryPoint": "Receive-CippHttpTrigger",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [
        "get",
        "post",
        "patch",
        "put",
        "delete",
        "options"
      ],
      "route": "{*CIPPEndpoint}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "name": "starter",
      "type": "durableClient",
      "direction": "in"
    }
  ]
}
